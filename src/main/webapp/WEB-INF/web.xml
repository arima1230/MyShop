<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>MyShop1</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.jsp</welcome-file>
    <welcome-file>default.htm</welcome-file>
  </welcome-file-list>
  <servlet>
    <servlet-name>StockServlet</servlet-name>
    <servlet-class>com.turna.servlet.adminstock.StockServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StockServlet</servlet-name>
    <url-pattern>/admin_stock</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>StockEditingServlet</servlet-name>
    <servlet-class>com.turna.servlet.adminstock.StockEditingServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>StockEditingServlet</servlet-name>
    <url-pattern>/admin_stock_editing</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>SalesServlet</servlet-name>
    <servlet-class>com.turna.servlet.adminsales.SalesServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>SalesServlet</servlet-name>
    <url-pattern>/admin_sales</url-pattern>
  </servlet-mapping>
</web-app>